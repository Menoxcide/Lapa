{
  "name": "lapa-core",
  "version": "1.2.0",
  "description": "LAPA Core - Local AI Pair Programmer Agent for Cursor",
  "main": "extension/dist/index.js",
  "type": "module",
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "lapa.startSwarm",
        "title": "LAPA: Start Swarm"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Menoxcide/Lapa.git"
  },
  "bugs": {
    "url": "https://github.com/Menoxcide/Lapa/issues"
  },
  "homepage": "https://github.com/Menoxcide/Lapa#readme",
  "keywords": [
    "ai",
    "cursor",
    "mcp",
    "ctx-zip",
    "nim",
    "moe",
    "swarm",
    "autogen",
    "roo-code"
  ],
  "author": "LAPA Team",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf extension/dist coverage .lapa/sandbox",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "build:watch": "tsc --watch",
    "test": "vitest",
    "test:log": "vitest --reporter=verbose --reporter=json > vitest-report.json 2>&1",
    "test:debug": "node --inspect-brk node_modules/vitest/vitest.mjs",
    "test:ui": "vitest 'src/__tests__/ui/**/*' --reporter=verbose",
    "test:watch": "vitest --watch",
    "test:ctx-zip": "vitest src/__tests__/ctx-zip.test.ts",
    "test:handoffs": "vitest src/__tests__/local/handoff-performance.local.spec.ts",
    "test:delegate": "vitest src/__tests__/swarm/delegate.local.spec.ts",
    "test:local": "npm run test:handoffs && npm run test:delegate",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "vsix": "npm run build && vsce package --no-dependencies",
    "vsix:install": "cmd /c \"\"C:\\Program Files\\Microsoft VS Code\\bin\\code\" --install-extension *.vsix --force\"",
    "setup:nim": "bash scripts/install-nim.sh",
    "dev": "npm run build:watch",
    "prepublishOnly": "npm run build && npm run test"
  },
  "files": [
    "extension/dist/",
    "media/",
    "cursor.json",
    "README.md",
    "LICENSE",
    "AGENT.md",
    "DOCUMENTATION.md",
    "LAPA_Master_Plan.toon",
    "LAPA_v1.2_TOON_SPEC.toon"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vscode/vsce": "^3.7.0",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.62",
    "@anthropic-ai/sdk": "^0.68.0",
    "@e2b/mcp-server": "^0.2.1",
    "@openai/agents": "^0.3.0",
    "@openai/agents-core": "^0.3.0",
    "@openai/agents-extensions": "^0.3.0",
    "@openai/agents-openai": "^0.3.0",
    "@openai/agents-realtime": "^0.3.0",
    "@toon-format/toon": "^0.8.0",
    "axios": "^1.13.2",
    "ctx-zip": "^0.0.9",
    "e2b": "^2.6.4",
    "ollama": "^0.6.2",
    "ollama-ai-provider": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7",
    "stripe": "^19.3.0",
    "uuid": "^13.0.0",
    "winget": "^0.0.1",
    "zod": "^3.23.8"
  }
}