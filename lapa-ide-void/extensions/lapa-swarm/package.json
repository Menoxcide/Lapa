{
  "name": "lapa-brain",
  "displayName": "LAPA Brain Extension",
  "description": "LAPA Core extension for LAPA IDE (Phase 2 ExtractPurity)",
    "version": "1.0.0",
  "publisher": "LAPA",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Menoxcide/Lapa.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lapa.swarm.start",
        "title": "LAPA: Start Swarm",
        "category": "LAPA Swarm"
      },
      {
        "command": "lapa.swarm.stop",
        "title": "LAPA: Stop Swarm",
        "category": "LAPA Swarm"
      },
      {
        "command": "lapa.swarm.pause",
        "title": "LAPA: Pause Swarm",
        "category": "LAPA Swarm"
      },
      {
        "command": "lapa.swarm.configure",
        "title": "LAPA: Configure Swarm",
        "category": "LAPA Swarm"
      },
      {
        "command": "lapa.swarm.status",
        "title": "LAPA: Show Swarm Status",
        "category": "LAPA Swarm"
      },
      {
        "command": "lapa.swarm.resume",
        "title": "LAPA: Resume Swarm",
        "category": "LAPA Swarm"
      },
      {
        "command": "lapa.swarm.upgrade",
        "title": "LAPA: Upgrade to Pro",
        "category": "LAPA Swarm",
        "icon": "$(star-full)"
      },
      {
        "command": "lapa.swarm.activateLicense",
        "title": "LAPA: Activate License",
        "category": "LAPA Swarm"
      },
      {
        "command": "lapa.git.generateCommit",
        "title": "LAPA: Generate Git Commit Message",
        "category": "LAPA Swarm",
        "icon": "$(git-commit)"
      },
      {
        "command": "lapa.swarm.restore",
        "title": "LAPA: Restore Session",
        "category": "LAPA Swarm",
        "icon": "$(history)"
      },
      {
        "command": "lapa.swarm.listSessions",
        "title": "LAPA: List Saved Sessions",
        "category": "LAPA Swarm",
        "icon": "$(list-unordered)"
      },
      {
        "command": "lapa.commandPalette.ai",
        "title": "LAPA: AI Command Search",
        "category": "LAPA Swarm",
        "icon": "$(search)"
      },
      {
        "command": "lapa.personas.list",
        "title": "LAPA: List Personas",
        "category": "LAPA Swarm",
        "icon": "$(person)"
      },
      {
        "command": "lapa.personas.reload",
        "title": "LAPA: Reload Personas",
        "category": "LAPA Swarm",
        "icon": "$(refresh)"
      },
      {
        "command": "lapa.workflow.generate",
        "title": "LAPA: Generate Workflow",
        "category": "LAPA Swarm",
        "icon": "$(workflow)"
      },
      {
        "command": "lapa.enhancePrompt",
        "title": "LAPA: Enhance Prompt",
        "category": "LAPA Swarm",
        "icon": "$(sparkle)"
      },
      {
        "command": "lapa.switchProvider",
        "title": "LAPA: Switch Inference Provider",
        "category": "LAPA Swarm",
        "icon": "$(sync)"
      },
      {
        "command": "lapa.settings.open",
        "title": "LAPA: Open Settings",
        "category": "LAPA Swarm",
        "icon": "$(settings)"
      },
      {
        "command": "lapa.marketplace.open",
        "title": "LAPA: Open MCP Marketplace",
        "category": "LAPA Swarm",
        "icon": "$(package)"
      },
      {
        "command": "lapa.dashboard.open",
        "title": "LAPA: Open Dashboard",
        "category": "LAPA Swarm",
        "icon": "$(dashboard)"
      },
      {
        "command": "lapa.roi.open",
        "title": "LAPA: Open ROI Widget",
        "category": "LAPA Swarm",
        "icon": "$(graph)"
      },
      {
        "command": "lapa.taskHistory.open",
        "title": "LAPA: Open Task History",
        "category": "LAPA Swarm",
        "icon": "$(history)"
      }
    ],
    "keybindings": [
      {
        "command": "lapa.swarm.start",
        "key": "ctrl+shift+s",
        "mac": "cmd+shift+s",
        "when": "editorTextFocus"
      },
      {
        "command": "lapa.swarm.stop",
        "key": "ctrl+shift+x",
        "mac": "cmd+shift+x",
        "when": "editorTextFocus"
      },
      {
        "command": "lapa.swarm.pause",
        "key": "ctrl+shift+p",
        "mac": "cmd+shift+p",
        "when": "editorTextFocus"
      },
      {
        "command": "lapa.swarm.configure",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c",
        "when": "editorTextFocus"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lapaSwarm",
          "title": "LAPA Swarm",
          "icon": "media/lapa-icon.svg"
        }
      ],
      "auxiliarybar": [
        {
          "id": "lapaSwarmAuxiliary",
          "title": "LAPA Swarm",
          "icon": "media/lapa-icon.svg"
        }
      ]
    },
    "views": {
      "lapaSwarm": [
        {
          "id": "lapaSwarmView",
          "name": "Swarm Dashboard",
          "when": "config.lapaSwarm.enabled"
        }
      ],
      "lapaSwarmAuxiliary": [
        {
          "id": "lapaSwarmAuxiliaryView",
          "name": "Swarm Auxiliary",
          "when": "config.lapaSwarm.auxiliaryEnabled"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "lapaSwarmView",
        "contents": "Welcome to LAPA Swarm!\n[Start Swarm](command:lapa.swarm.start)"
      }
    ],
    "configuration": {
      "title": "LAPA Swarm",
      "properties": {
        "lapaSwarm.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable LAPA Swarm extension"
        },
        "lapaSwarm.auxiliaryEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable LAPA Swarm auxiliary view"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./tsconfig.json",
    "watch": "tsc -watch -p ./tsconfig.json",
    "build:webview": "webpack --mode production",
    "watch:webview": "webpack --mode development --watch",
    "build": "npm run compile && npm run build:webview",
    "watch:all": "npm run watch & npm run watch:webview",
    "package": "vsce package --no-dependencies --out lapa-brain.vsix",
    "publish": "vsce publish --no-dependencies",
    "test": "vitest run --coverage",
    "test:extract": "vitest run --coverage --coverage.thresholds.lines=95 --coverage.thresholds.functions=95 --coverage.thresholds.branches=95 --coverage.thresholds.statements=95"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.62",
    "@anthropic-ai/sdk": "^0.68.0",
    "@e2b/mcp-server": "^0.2.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@openai/agents": "^0.3.0",
    "@openai/agents-core": "^0.3.0",
    "@openai/agents-extensions": "^0.3.0",
    "@openai/agents-openai": "^0.3.0",
    "@openai/agents-realtime": "^0.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@toon-format/toon": "^0.8.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "axios": "^1.13.2",
    "chromadb": "^3.1.2",
    "ctx-zip": "^0.0.9",
    "dot": "^1.1.3",
    "e2b": "^2.6.4",
    "gif.js": "^0.2.0",
    "graphviz": "^0.0.9",
    "html2canvas": "^1.4.1",
    "jsdom": "^27.2.0",
    "kubo-rpc-client": "^5.0.0",
    "langsmith": "^0.1.0",
    "ollama": "^0.6.2",
    "ollama-ai-provider": "^1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sqlite3": "^5.1.7",
    "stripe": "^19.3.0",
    "systeminformation": "^5.27.11",
    "uuid": "^13.0.0",
    "vitest": "^4.0.9",
    "winget": "^0.0.1",
    "ws": "^8.18.0",
    "js-yaml": "^4.1.0",
    "yaml": "^2.4.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5",
    "docx": "^8.5.0",
    "pptxgenjs": "^3.12.0",
    "xlsx": "^0.18.5",
    "cohere-ai": "^7.0.0",
    "franc": "^6.1.0",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.0",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^3.7.0",
    "@playwright/test": "^1.50.0",
    "typescript": "^5.9.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "esbuild-loader": "^4.3.0",
    "style-loader": "^4.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0"
  }
}